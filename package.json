{
  "name": "biljoenbad",
  "version": "1.0.0",
  "main": "gulpfile.js",
  "author": "Emotion",
  "copyright": "Emotion",
  "license": "UNLICENSED",
  "private": true,
  "paths": {
    "src": {
      "base": "src/",
      "scss": "src/scss/",
      "fonts": "src/fonts/",
      "sw": "src/serviceworker/",
      "images": {
        "base": "src/images/",
        "bitmap": {
          "base": "src/images/bitmap/"
        },
        "svg": {
          "base": "src/images/svg/",
          "single": "src/images/svg/single/",
          "sprite": "src/images/svg/sprite/"
        }
      },
      "json": "src/json/",
      "js": "src/js/"
    },
    "dist": {
      "base": "./public/",
      "assets": "./public/assets/",
      "critical": "./public/assets/criticalcss/",
      "css": "./public/assets/css/",
      "js": "./public/assets/js/",
      "fonts": "./public/assets/fonts/",
      "favicon": "./public/assets/images/favicon/",
      "images": {
        "base": "./public/assets/images/",
        "bitmap": {
          "base": "./public/assets/images/bitmap/"
        },
        "svg": {
          "base": "./public/assets/images/svg/",
          "single": "./public/assets/images/svg/single/",
          "sprite": "./public/assets/images/svg/sprite/"
        }
      },
      "manifest": "./public/assets/manifests/"
    },
    "abs": {
      "js": "/assets/js/"
    },
    "build": {
      "base": "./build/",
      "css": "./build/css/",
      "fonts": "./build/fonts/",
      "js": "./build/js/",
      "html": "./build/html/",
      "img": "./build/img/"
    },
    "favicon": {
      "src": "./src/images/favicon/favicon_src.{svg,png}",
      "dest": "./public/assets/images/favicon/",
      "path": "./assets/images/favicon/"
    },
    "craftConfig": "./config/",
    "templates": {
      "base": "./templates/",
      "inlineJs": "./templates/_inline/_js/",
      "inlineSvg": "./templates/_inline/_svg/",
      "criticalCss": "./templates/_inline/_critical/"
    }
  },
  "urls": {
    "live": "https://biljoenbad.nl/",
    "local": "biljoenbad.loc",
    "critical": "https://biljoenbad.nl/"
  },
  "vars": {
    "siteCssName": "styles.min.css",
    "scssName": "style.scss",
    "scssBootstrapComponentsName": "bootstrap-components.scss",
    "siteCssBootstrapComponentsName": "bootstrap-components.min.css",
    "jsName": "main.js",
    "jsNameLegacy": "main-legacy.js",
    "manifest": {
      "legacy": "manifest-legacy.json",
      "modern": "manifest.json"
    },
    "webapp": {
      "theme_color": "#fff",
      "background_color": "#fff",
      "display": "standalone",
      "orientation": "any",
      "scope": "/",
      "start_url": "/",
      "lang": "nl"
    }
  },
  "globs": {
    "components": [],
    "fonts": [
      "./src/fonts/*.{eot,ttf,woff,woff2,svg}"
    ],
    "favicon": [
      "./src/images/favicon/*.{png,xml,ico,json,svg}"
    ],
    "purgecss": [
      "./templates/**/*.{html,twig}",
      "./public/assets/js/**/*.js",
      "./public/*.html",
      "./src/js/**/*.js"
    ],
    "purgecssWhitelist": [],
    "critical": [
      {
        "url": "",
        "template": "index"
      },
      {
        "url": "offline",
        "template": "offline"
      },
      {
        "url": "error",
        "template": "error"
      },
      {
        "url": "503",
        "template": "503"
      },
      {
        "url": "404",
        "template": "404"
      }
    ],
    "criticalWhitelist": [],
    "a11y": [
      {
        "url": ""
      }
    ],
    "download": [
      {
        "url": "https://www.google-analytics.com/analytics.js",
        "dest": "./public/assets/js/"
      }
    ],
    "copyVendorToDistJs": [
      "./node_modules/picturefill/dist/picturefill.min.js",
      "./node_modules/svgxuse/svgxuse.js",
      "./node_modules/lazysizes/lazysizes.min.js"
    ],
    "lintJs": [
      "./src/js/**/*.js",
      "!./src/js/{vendor, dev}/*"
    ],
    "inlineJs": [
      "./node_modules/fg-loadcss/dist/cssrelpreload.js",
      "./node_modules/tiny-cookie/dist/tiny-cookie.min.js",
      "./src/js/_inline/**/*.js",
      "./node_modules/fullcalendar-scheduler/main.min.js",
      "./node_modules/fullcalendar/locales/nl.js"
    ],
    "revision": {
      "files": [
        "./public/assets/images/**/*.*"
      ],
      "replace": [
        "./templates/**/*.{html,twig,css,js,svg}",
        "./public/assets/**/*.{css,js}",
        "./public/*.{html,json,js}"
      ]
    }
  },
  "dependencies": {
    "@fullcalendar/core": "^5.1.0",
    "@fullcalendar/daygrid": "^5.1.0",
    "@fullcalendar/resource-timegrid": "^5.1.0",
    "@typeform/embed": "^0.22.2",
    "autotrack": "^2.4.1",
    "baguettebox.js": "^1.11.1",
    "css-loader": "^3.6.0",
    "fullcalendar": "^5.1.0",
    "fullcalendar-scheduler": "^5.1.0",
    "lazysizes": "^2.0.6",
    "loadjs": "^3.3.1",
    "picturefill": "^3.0.2",
    "remove": "^0.1.5",
    "static-website": "git+ssh://git@git.emotion.nl:emotion/bootstrap-components.git",
    "svgxuse": "^1.2.6",
    "tiny-cookie": "^2.1.2",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "@glidejs/glide": "^3.3.0",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "bootstrap-components": "git+ssh://git@git.emotion.nl:emotion/bootstrap-components.git",
    "breakpoint-sass": "^2.7.1",
    "browser-sync": "^2.26.5",
    "chalk": "^2.4.2",
    "core-js": "2",
    "critical": "^2.0.0-9",
    "cssnano": "^4.1.8",
    "ecssential": "git+ssh://git@git.emotion.nl:emotion/ecssential.git",
    "eslint": "^5.16.0",
    "fancy-log": "^1.3.3",
    "fg-loadcss": "^2.1.0",
    "fibers": "^4.0.1",
    "fs-extra": "8.0.1",
    "git-user-name": "^2.0.0",
    "glob": "^7.1.3",
    "gulp": "^4.0.2",
    "gulp-cached": "^1.1.1",
    "gulp-concat": "^2.6.0",
    "gulp-download": "^0.0.1",
    "gulp-eslint": "^5.0.0",
    "gulp-filter": "^5.1.0",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "5.0.3",
    "gulp-load-plugins": "^1.5.0",
    "gulp-newer": "^1.2.0",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-print": "^2.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "0.5.4",
    "gulp-rev": "^9.0.0",
    "gulp-rev-rewrite": "^1.1.3",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.2.1",
    "gulp-streamify": "1.0.2",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-uglify": "^1.5.4",
    "gzip-size": "^5.0.0",
    "idlize": "^0.1.0",
    "imagemin-giflossy": "^5.1.10",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^7.0.0",
    "imagemin-pngquant": "^7.0.0",
    "imagemin-svgo": "^7.0.0",
    "imagemin-zopfli": "^6.0.0",
    "lighthouse": "^5",
    "log-symbols": "^2.2.0",
    "modularscale-sass": "^2.1.1",
    "node-sass-magic-importer": "^5.3.1",
    "normalize.css": "^8.0.1",
    "pa11y": "^5.1.0",
    "pa11y-reporter-html": "^1.0.0",
    "postcss": "^7.0.16",
    "postcss-reporter": "^6.0.1",
    "pretty-bytes": "^5.1.0",
    "puppeteer": "^1.17.0",
    "require-dir": "^1.1.0",
    "rev-hash": "^2.0.0",
    "rev-path": "^2.0.0",
    "sass": "^1.20.1",
    "sitemapper": "^2.1.14",
    "slack": "^11.0.2",
    "stylelint": "^10.0.1",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-prettier": "^1.1.0",
    "stylelint-scss": "^3.6.1",
    "susy": "^3.0.5",
    "terser-webpack-plugin": "^1.3.0",
    "typesize": "git+ssh://git@git.emotion.nl:emotion/typesize.git",
    "webpack": "^4.29.6",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1",
    "workbox-build": "^4.3.1",
    "yargs": "^13.2.4"
  },
  "resolutions": {
    "**/puppeteer": "^2.0.0"
  },
  "scripts": {
    "start": "gulp",
    "build": "gulp build"
  },
  "bugs": {
    "email": "interactie@emotion.nl"
  },
  "browserslist": [
    "> 1%",
    "ie >= 11",
    "last 3 versions"
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 6
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-else-return": 2,
      "eqeqeq": 2,
      "no-multi-spaces": 2,
      "no-loop-func": 2,
      "no-use-before-define": 2
    }
  },
  "stylelint": {
    "syntax": "scss"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "corejs": {
            "version": 2,
            "proposals": true
          },
          "modules": false,
          "useBuiltIns": "usage"
        }
      ]
    ]
  }
}
