{
  "require": {
    "aelvan/imager": "^2.4",
    "craftcms/cms": "^3.4.0",
    "craftcms/redactor": "2.6.0.1",
    "nystudio107/craft-cookies": "^1.1",
    "nystudio107/craft-minify": "^1.2",
    "nystudio107/craft-seomatic": "^3.2",
    "nystudio107/craft-twigpack": "^1.2",
    "nystudio107/craft-typogrify": "^1.1",
    "ostark/craft-async-queue": "~1.3",
    "sebastianlenz/linkfield": "1.0.20",
    "verbb/image-resizer": "^2.0",
    "verbb/navigation": "^1.3",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
